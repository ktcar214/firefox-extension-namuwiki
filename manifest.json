{

    "manifest_version": 2,
    "name": "Search namu.wiki",
    "description": "Adds a search engine that searches namu.wiki",
    "version": "1.0",
    "icons": {
      "256": "icons/namuwiki.png"
    },

    "browser_specific_settings": {
      "gecko": {
        "strict_min_version": "67.0",
        "id": "NamuWikiSearchEngine@ktcar214"
      }
    },

    "chrome_settings_overrides": {
      "search_provider": {
        "name": "namu.wiki(나무위키)",
        "search_url": "https://namu.wiki/go/{searchTerms}",
        "keyword": "@namu",
        "favicon_url": "http://namu.wiki/favicon.ico",
        "is_default": false,
        "encoding": "UTF-8"
      }
    },
    "author": "ktcar214",
    "homepage_url": "https://github.com/ktcar214/firefox-extension-namuwiki"

}
  
